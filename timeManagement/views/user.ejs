                                          <div class="col">
                                              <form method="post">
                                                  <div class="form-row">
                                                      <div class="form-group col-md-6">
                                                          <label for="feFirstName">Email</label>
                                                          <input type="text" name="username" placeholder="Email" class="form-control"> </div>
                                                      <div class="form-group col-md-6">
                                                          <label for="feEmailAddress">Employee ID</label>
                                                          <input type="text" name="employeeID" placeholder="Employee ID" class="form-control">
                                                          
                                                       </div>
                                                  </div>
                                                  <div class="form-row">
                                                      <div class="form-group col-md-6">
                                                          <label for="feLastName">Last Name</label>
                                                          <input type="text" name="lastName" placeholder="LastName" class="form-control"></div>
                                                      <div class="form-group col-md-6">
                                                          <label for="fePhone">First Name</label>
                                                          <input type="text" name="firstName" placeholder="First Name" class="form-control"> </div>
                                                          
                                                        </div>
                                                  
                                                  <div class="form-row">
                                                      <div class="form-group col-md-6">
                                                          <label for="feEmpolyeeID">Department</label>
                                                          <input type="text" name="department" placeholder="Department" class="form-control"></div>
                                                      <div class="form-group col-md-6">
                                                          <label for="feJobTitle">Job Title</label>
                                                          <input type="text" name="jobTitle" placeholder="Job Title" class="form-control"> </div>
                                                  </div>
                                                  <div class="form-row">
                                                      <div class="form-group col-md-6">
                                                          <label for="feEmpolyeeID">Level</label>
                                                          <input type="text" name="level" placeholder="Level" class="form-control"> </div>
                                                      <div class="form-group col-md-6">
                                                          <label for="feJobTitle">Employment Duration</label>
                                                          <input type="text" name="duration" placeholder="Employment Duration" class="form-control"> </div>
                                                  </div>
                                                  <input type="submit" value="Add User" class="btn btn-success">
